    "import pyttsx3\n",
    "txt_sp=pyttsx3.init()\n",
    "text=input(\"enter a text...\\n\")\n",
    "voices=txt_sp.getProperty('voices')\n",
    "for voice in voices:\n",
    "    print(\"ID:\",voice.id)\n",
    "txt_sp.setProperty('voice',voices[1].id)    \n",
    "txt_sp.say(text)\n",
    "txt_sp.runAndWait()"
